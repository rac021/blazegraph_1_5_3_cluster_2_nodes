    <build>
        <plugins>
         <plugin>
           <artifactId>maven-antrun-plugin</artifactId>
           <version>1.6</version>
           <executions>
             <execution>
               <goals>
                 <goal>run</goal>
               </goals>
               <phase>process-resources</phase>
               <configuration>
                 <target>
                   <property name="build.ver" value="${project.version}"/>
                   <property name="build.ver.osgi" value="${project.version}"/>
                   <property name="snapshot" value="false"/>
                   <ant antfile="build.xml" target="clean"/>
                   <ant antfile="build.xml" target="jar"/>
                 </target>
               </configuration>
             </execution>
             <execution>
                  <id>attach-artifact</id>
                  <phase>package</phase>
                  <goals>
                    <goal>run</goal>
                  </goals>
                  <configuration>
                    <target>
                      <attachartifact file="ant-build/bigdata-${project.version}.jar" type="jar"/>
                    </target>
                  </configuration>
                </execution>
           </executions>
         </plugin>
        </plugins>
